
# Listing of Parameters
# ---------------------
subsection Alphas
  # Any constant used inside the function which is not a variable name.
  set Function constants  = 

  # Separate vector valued expressions by ';' as ',' is used internally by the
  # function parser.
  set Function expression =  0.1*exp(cosh(x)^(-1.0) * cosh(y)^(-1.0) ) ; 0.0 ;  0.10*cos(x)*cos(y) #1.0 + 0.8 *exp((-((x+0.50)^2.0 + (y+0.50)^2.0 ) / 3.75)) #; 1.0


  # The name of the variables as they will be used in the function, separated
  # by ','. + 1.0*exp((-((x)^2.0 + (y+5.0)^2.0 ) / 2.0));  exp((-((x)^2.0 + (y)^2.0 ) / 2.0)) ; 0.001*cos(x)*cos(y); exp((-( (x)^2.0 + (y)^2.0 ) / 2.0))
  set Variable names      = x,y,t
end


